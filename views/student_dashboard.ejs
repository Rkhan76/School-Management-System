<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Student Dashboard</title>
    <link rel="stylesheet" href="./css/dashboard/studentDashboard.css" />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Montserrat&family=Poppins:wght@300&display=swap"
      rel="stylesheet"
    />
  </head>

  <body>
    <!-- sidebar content -->
    <div class="sidebar">
      <div class="sideNavbar">
        <div class="headerLogo">
          <img
            src="./images/social_media_logos/school_logo.jpg"
            alt="school_logo"
          />
        </div>
        <div class="toggleButton">
          <button type="submit">
            <img
              src="./images/student_dashboard/toggle-button.png"
              alt="toggle-button"
            />
          </button>
        </div>
      </div>
      <div class="dashboard sidebarSections">
        <div class="dashboard-icon icon">
          <img src="./images/student_dashboard/dashboard_icon.svg" alt="" />
        </div>
        <div>Dashboard</div>
        <div class="button">></div>
      </div>
      <div class="profile sidebarSections">
        <div class="profile-icon icon">
          <img src="./images/student_dashboard/profile.svg" alt="" />
        </div>
        <div>Profile</div>
        <div class="button">></div>
      </div>
      <div class="Attendence sidebarSections">
        <div class="Attendence-icon icon">
          <img src="./images/student_dashboard/attendance.svg" alt="" />
        </div>
        <div>Attendance</div>
        <div class="button">></div>
      </div>
      <div class="time-table sidebarSections">
        <div class="time-table-icon icon">
          <img src="./images/student_dashboard/time-table.svg" alt="" />
        </div>
        <div>Time Table</div>
        <div class="button">></div>
      </div>
      <div class="result sidebarSections">
        <div class="result-icon icon">
          <img src="./images/student_dashboard/result.svg" alt="" />
        </div>
        <div>Result</div>
        <div class="button">></div>
      </div>
      <div class="admit-card sidebarSections">
        <div class="admit-card-icon icon">
          <img src="./images/student_dashboard/admit-card.svg" alt="" />
        </div>
        <div>Admit Card</div>
        <div class="button">></div>
      </div>
      <div class="logout sidebarSections">
        <div class="dashboard-symbol icon">
          <img src="./images/student_dashboard/logout.svg" alt="" />
        </div>
        <div>Logout</div>
        <div class="button" type="submit">></div>
      </div>
    </div>


    <!-- left container for main content -->
    <div class="main-container">
      <div class="navbar">
        <div class="search-bar">
          <div class="search-button" type="submit">
            <img
              src="./images/student_dashboard/search-icon.svg"
              alt="search-icon"
            />
          </div>
          <div class="text-input"><input type="text" placeholder="Find Something..." /></div>
        </div>
        <div class="second-bar">
          <div class="user-profile">
            <div class="username">
              <h4>Stevene Zone</h4>
              <p>Admin</p>
            </div>
            <div class="dropdown-symbol">
              <img src="./images/student_dashboard/dropdown.svg" alt="" />
            </div>
            <div class="user-profile-pic">
              <img src="./images/student_dashboard/user-profile.png" alt="" />
            </div>
          </div>
          <div class="messages">
            <div class="number">8</div>
            <img src="./images/student_dashboard/message.svg" alt="" />
          </div>
          <div class="notifications">
            <div class="number">5</div>
            <img src="./images/student_dashboard/notification-bell.svg" alt="" />
          </div>
        </div>
      </div>
      <div class="main-content">
        <div class="dashboardName">
          <h1>Admin Dashboard</h1>
          <div class="routeName">
            <div>Home</div>
            <div>></div>
            <div>Student</div>
          </div>
        </div>
        <div class="detailContainer">
          <div class="leftPart">
            <div class="aboutMe"></div>
          </div>
          <div class="rightPart">
            <div class="khan"></div>
          </div>
       
        </div>
      </div>
    </div>
  </body>
</html>
